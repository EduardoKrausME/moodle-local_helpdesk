                    "html_before": "",
                    "html_after": "",
                    "commandsql": "SELECT u.id AS userid,\r\n       u.firstname,\r\n       u.lastname,\r\n       u.email,\r\n       COUNT(t.id) AS total_tickets\r\n  FROM mdl_local_khelpdesk_ticket t\r\n  JOIN mdl_user u ON t.userid = u.id\r\nGROUP BY u.id, u.firstname, u.lastname, u.email\r\nORDER BY total_tickets DESC\r\n   LIMIT 10",
                    "cache": "1d",
                    "reload": "none",
                    "info": "{\n    \"column\": {\n        \"userid\": {\n            \"key\": \"userid\",\n            \"title\": \"lang::report_user_reports_users_with_most_tickets_open_userid::local_khelpdesk\",\n            \"type\": \"userphotoRenderer\",\n            \"mustache\": \"{{{userid}}}\"\n        },\n        \"firstname\": {\n            \"key\": \"firstname\",\n            \"title\": \"lang::u_fullname::local_kbi\",\n            \"type\": \"userfullname\",\n            \"mustache\": \"<a href=\\\"{{{config.wwwroot}}}\\\/user\\\/view.php?id={{{u_id}}}\\\"\\r\\n   target=\\\"profile\\\">{{{u_fullname}}}<\\\/a>\"\n        },\n        \"lastname\": {\n            \"key\": \"lastname\",\n            \"title\": \"lastname\",\n            \"type\": \"none\",\n            \"mustache\": \"\"\n        },\n        \"email\": {\n            \"key\": \"email\",\n            \"title\": \"lang::email::moodle\",\n            \"type\": \"string\",\n            \"mustache\": \"<a href=\\\"mailto:{{{email}}}\\\" target=\\\"_blank\\\">{{{email}}}<\\\/a>\"\n        },\n        \"total_tickets\": {\n            \"key\": \"total_tickets\",\n            \"title\": \"lang::report_general_reports_tickets_created_by_period_total_tickets::local_khelpdesk\",\n            \"type\": \"number\",\n            \"mustache\": \"{{{total_tickets}}}\"\n        }\n    }\n}"
