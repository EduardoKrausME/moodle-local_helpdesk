                    "title": "lang::report_user_reports_avg_response_time_by_user_cat_title::local_khelpdesk",
                    "block_num": 1,
                    "type": "table",
                    "theme": "light",
                    "css": "",
                    "html_before": "",
                    "html_after": "",
                    "commandsql": "SELECT \r\n    u.id AS userid, \r\n    u.firstname, \r\n    u.lastname, \r\n    AVG(r.createdat - t.createdat) AS avg_response_time_seconds\r\nFROM mdl_local_khelpdesk_ticket t\r\nJOIN mdl_local_khelpdesk_response r ON t.id = r.ticketid\r\nJOIN mdl_user u ON r.userid = u.id\r\nWHERE r.createdat = (\r\n    SELECT MIN(r2.createdat) \r\n    FROM mdl_local_khelpdesk_response r2 \r\n    WHERE r2.ticketid = t.id\r\n)\r\nGROUP BY u.id, u.firstname, u.lastname\r\nORDER BY avg_response_time_seconds ASC",
                    "cache": "1d",
                    "reload": "none",
                    "info": "{\n    \"column\": {\n        \"userid\": {\n            \"key\": \"userid\",\n            \"title\": \"lang::report_user_reports_users_with_most_tickets_open_userid::local_khelpdesk\",\n            \"type\": \"userphotoRenderer\",\n            \"mustache\": \"{{{userid}}}\"\n        },\n        \"firstname\": {\n            \"key\": \"firstname\",\n            \"title\": \"lang::u_fullname::local_kbi\",\n            \"type\": \"userfullname\",\n            \"mustache\": \"<a href=\\\"{{{config.wwwroot}}}\\\/user\\\/view.php?id={{{u_id}}}\\\"\\r\\n   target=\\\"profile\\\">{{{u_fullname}}}<\\\/a>\"\n        },\n        \"lastname\": {\n            \"key\": \"lastname\",\n            \"title\": \"lastname\",\n            \"type\": \"none\",\n            \"mustache\": \"\"\n        },\n        \"avg_response_time_seconds\": {\n            \"key\": \"avg_response_time_seconds\",\n            \"title\": \"lang::report_general_reports_avg_resolution_time_avg_resolution_time_hours::local_khelpdesk\",\n            \"type\": \"datetimeRenderer\",\n            \"mustache\": \"\"\n        }\n    }\n}"
